<?xml version="1.0"?>
<configuration>
    <location path="Register.aspx">
        <system.web>
            <authorization>
                <allow users="*" />
            </authorization>
        </system.web>
    </location>

    <location path="ForgotPassword.aspx">
        <system.web>
            <authorization>
                <allow users="*" />
            </authorization>
        </system.web>
    </location>

    <location path="Verify.aspx">
        <system.web>
            <authorization>
                <allow users="*" />
            </authorization>
        </system.web>
    </location>

    <system.web>
        <authorization>
            <deny users="?" />
        </authorization>
    </system.web>
</configuration>
